<!DOCTYPE html>
<html>
<head>
  <title>Pallets</title>
  <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <%- include('partials/header') %>
  <div class="container">
    <h1>Confirmed Transfer orders</h1>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Bin #</th>
          <th>SKU Code</th>
          <th>SKU Desc</th>
          <th>SUT</th>
          <th>pallet id</th>
          <th>Pallet Qty</th>
          <th>Bin Capacity</th>
          <th>Total pallets</th>
        </tr>
      </thead>
      <tbody>
        <% pallets.forEach(pallet => { %>
          <tr>
            <td><%= pallet.date %></td>
            <td><%= pallet.skuCode %></td>
            <td><%= pallet.skuDesc %></td>
            <td><%= pallet.skuCode %></td>
            <td><%= pallet.palletQty %></td>
            <td><%= pallet.palletQty %></td>
            <td><%= pallet.transferOrder %></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>         
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
  <%- include('partials/footer') %>

  <script src="/scripts.js"></script>
</body>
</html>
